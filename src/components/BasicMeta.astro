---
export type MetaProps = {
    description?: string,
    title?: string
}
const {description, title} = Astro.props as MetaProps
---
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="preload" href="https://fonts.goodbyte.ca/editorial-new.woff2" as="font" type="font/woff2" crossorigin>
<meta name="author" content="Jack Guinane, Quinn Pollock"/>
<title>{title ?? "Goodbyte Games"}</title>
<meta name="description" content={description ?? "Goodbyte Game: Sometimes we make games, and when we do, they go here. "}/>
<link rel="icon" type="image/svg+xml" href="/favicon.svg">
<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#00ad41">
